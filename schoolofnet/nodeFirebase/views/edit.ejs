<%- include('./layout/head') %>
    <h3>Listagem</h3>

    <form action="/edit/<%= id %>" method="POST">
       
       <div class="row">
        <div class="input-field col s3">
            <input id="name" name="name" type="text" value="<%= contacts.name %>">
            <label for="name">Nome</label>
        </div></div>
        
        <div class="row">
        <div class="input-field col s3">
            <input id="email" type="text" name="email" value="<%= contacts.email %>">
            <label for="email">E-mail</label>
        </div></div>

         <p><input type="submit" value="Salvar" class="btn"> <a href="/view/<%= id %>" class="btn red">Voltar</a></p> 

    </form>

  


    <%- include('./layout/footer') %>